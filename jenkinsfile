node {
	stage('Checkout code') {
        steps {
            checkout scm
        }
    }
	stage('Build'){
		echo "build pass"
		sh "mvn -version"
        sh "mvn clean"
	}
	stage('test'){
		echo 'test pass'
		sh "mvn test"
	}
	stage('Compile'){
		echo 'Compile pass'
		sh "mvn compile"
	}
	stage('Deploy'){
		echo "Deploying the Project.........."
	}
}